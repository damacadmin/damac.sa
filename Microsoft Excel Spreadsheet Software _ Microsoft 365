  .carousel-item { transition: none 0s ease 0s; }
}

.carousel-item:focus { outline: currentcolor dotted 0.1875rem; }

.carousel-item:focus:not(:focus-visible) { outline: 0px; }

.carousel-item:focus-visible { outline: currentcolor dotted 0.1875rem; }

.carousel-item.get-height { display: block; visibility: hidden; opacity: 0; }

.carousel-item-next, .carousel-item-prev, .carousel-item.active { display: block; }

.active.carousel-item-right, .carousel-item-next:not(.carousel-item-left) { transform: translateX(100%); }

.active.carousel-item-left, .carousel-item-prev:not(.carousel-item-right) { transform: translateX(-100%); }

