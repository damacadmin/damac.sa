.bg-glass-var { backdrop-filter: blur(var(--backdrop-filter-blur, 3px)); background: rgba(var(--glass-bg-color,255,255,255),var(--glass-background-opacity,0.3)); border-color: rgba(var(--glass-border-color,255,255,255),var(--glass-border-opacity,0.2)); color: var(--glass-text-color,#fff); transition: all 280ms ease-in-out 0s; }

@media (prefers-reduced-motion: reduce) {
  .bg-glass-var { transition: none 0s ease 0s; }
}

