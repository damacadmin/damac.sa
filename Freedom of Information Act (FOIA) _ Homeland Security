.grid-col-fill { flex: 1 1 0%; min-width: 1px; }

.grid-col-1, .grid-col-2 { flex: 0 1 auto; width: 8.33333%; }

.grid-col-2 { width: 16.6667%; }

.grid-col-3, .grid-col-4 { flex: 0 1 auto; width: 25%; }

.grid-col-4 { width: 33.3333%; }

.grid-col-5, .grid-col-6 { flex: 0 1 auto; width: 41.6667%; }

.grid-col-6 { width: 50%; }

.grid-col-7, .grid-col-8 { flex: 0 1 auto; width: 58.3333%; }

.grid-col-8 { width: 66.6667%; }

.grid-col-10, .grid-col-9 { flex: 0 1 auto; width: 75%; }

.grid-col-10 { width: 83.3333%; }

.grid-col-11, .grid-col-12 { flex: 0 1 auto; width: 91.6667%; }

.grid-col-12 { width: 100%; }

